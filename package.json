{
  "name": "@daicode/boiler-plate",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "test": "jest",
  "scripts": {
    "generate:version": "genversion -e src/version.ts",
    "build": "yarn generate:version && rm -rf dist && tsc --p tsconfig.build.json -d",
    "deploy:dev": "yarn build && sls deploy --stage dev --config serverless.yml",
    "deploy:prod": "yarn build && sls deploy --stage prod --config serverless.yml",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "serve": "export ENV=local && nest start --watch --debug",
    "test": "jest",
    "test:cov": "yarn jest --coverage",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/typeorm": "^10.0.0",
    "@vendia/serverless-express": "^4.10.4",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dynamoose": "^3.2.1",
    "moment": "^2.29.4",
    "mysql2": "^3.6.1",
    "nestjs-dynamoose": "^0.5.5",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.427.0",
    "@aws-sdk/client-sns": "^3.427.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/testing": "^10.2.7",
    "@types/aws-lambda": "^8.10.124",
    "@types/aws-serverless-express": "^3.3.6",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "genversion": "^3.1.1",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "serverless": "^3.35.2",
    "serverless-domain-manager": "^7.1.2",
    "serverless-layers": "^2.6.1",
    "serverless-offline": "^13.2.0",
    "serverless-plugin-aws-alerts": "^1.7.5",
    "serverless-plugin-canary-deployments": "^0.8.0",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-plugin-warmup": "^8.2.1",
    "serverless-prune-plugin": "^2.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
